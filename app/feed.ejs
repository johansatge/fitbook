<% for(let i = 0; i < 4; i += 1) { %>
  <h2 class="feed-title">Friday, April 12th</h2>
  <div class="feed-card">
    <% for(let j = 0; j < 4; j += 1) { %>
      <div class="feed-card-line <%= j % 2 !== 0 ? 'feed-card-line--odd' : '' %>">
        <div class="feed-card-line-icon">
          <svg class="svg-icon"><use href="#svg-fitness_center"></use></svg>
        </div>
        <button class="feed-card-toggle">
          <svg class="svg-icon"><use href="#svg-keyboard_arrow_down"></use></svg>
        </button>
        <h3 class="feed-card-title">Abs (crunch machine)</h3>
        <p class="feed-card-text">
          Weight: 68kg (08:35)
        </p>
        <% if (j === 0) { %>
          <table class="feed-card-data" cellpadding="0" cellspacing="0">
            <tr class="feed-card-data-line">
              <td class="feed-card-data-cell"><svg class="svg-icon"><use href="#svg-map"></use></svg></td>
              <td class="feed-card-data-cell">Distance</td>
              <td class="feed-card-data-cell">20km</td>
            </tr>
            <tr class="feed-card-data-line">
              <td class="feed-card-data-cell"><svg class="svg-icon"><use href="#svg-alarm_on"></use></svg></td>
              <td class="feed-card-data-cell">Duration</td>
              <td class="feed-card-data-cell">30mn</td>
            </tr>
            <tr class="feed-card-data-line">
              <td class="feed-card-data-cell"><svg class="svg-icon"><use href="#svg-event"></use></svg></td>
              <td class="feed-card-data-cell">Date</td>
              <td class="feed-card-data-cell">Thursday, April 18th (08:21)</td>
            </tr>
          </table>
        <% } %>
      </div>
    <% } %>
  </div>
<% } %>
